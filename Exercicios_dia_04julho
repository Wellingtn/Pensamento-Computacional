def gerar_intervalo_incrementado(n, i):
    for c in range(0, n+1, i):
        if c != n:
            print(c, end = ', ')
        else:
            print(c, end='. ')


def main():
    n = int(input('Digite um valor para ser o limite do intervalo: '))
    i = int(input('Digite outro valor para ser o passo do intervalo: '))
    gerar_intervalo_incrementado(n, i)


main()
